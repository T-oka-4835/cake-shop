<h4>会員一覧</h4>

<table>
  <tr>
    <th>会員ID</th>
    <th>氏名</th>
    <th>メールアドレス</th>
    <th>ステータス</th>
  </tr>
  <tbody>
    <% @customers.each do |customer| %>
      <tr>
        <td>
          <%= customer.id %>
        </td>
        <td>
          <%= link_to customer.last_name + customer.first_name, admins_customer_path(customer)  %>
        </td>
        <td>
          <%= customer.email %>
        </td>
        <td>
          ステータス
        </td>
      </tr>
    <% end %>
  </tbody>
</table>