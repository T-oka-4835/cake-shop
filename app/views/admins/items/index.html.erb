<h4>商品一覧</h4>
<%= link_to "商品追加", new_admins_item_path %>

<table>
  <thead>
    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜価格</th>
      <th>ジャンル</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.id %></td>
        <td><%= link_to item.name, admins_item_path(item) %></td>
        <td><%= item.price %></td>
        <td><%= item.name %></td>
        <td>サンプル</td>
      </tr>
    <% end %>
  </tbody>
</table>